{
  "name": "jest-go-to-test",
  "displayName": "Jest Go to Test",
  "repository": {
    "url": "https://github.com/Maxim-Filimonov/jest-go-to-test-vscode"
  },
  "description": "Switch between code and test in Jest",
  "version": "0.0.1",
  "publisher": "Maxim-Filimonov",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:extension.jestGoToTest"],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.jestGoToTest",
        "title": "Go to Test"
      }
    ],
    "keybindings": [
      {
        "command": "extension.jestGoToTest",
        "key": "ctrl+shift+y",
        "mac": "cmd+shift+y",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "test": "ava",
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "typescript": "^1.8.5",
    "vscode": "^0.11.12"
  },
  "ava": {
    "files": ["out/src/*.test.js"]
  }
}
